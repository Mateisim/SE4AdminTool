<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.SepiaTone?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<SplitPane fx:id="paneId" dividerPositions="0.18433179723502305, 0.5990783410138248, 0.6835637480798771, 0.9231950844854071" orientation="VERTICAL" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tfa.se4.gui.ServerPaneController">
  <items>
      <HBox SplitPane.resizableWithParent="false">
         <children>
            <VBox>
               <children>
                  <Label text="Map" VBox.vgrow="NEVER">
                     <padding>
                        <Insets left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin>
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Label>
                  <Label text="Mode" VBox.vgrow="NEVER">
                     <padding>
                        <Insets left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin>
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Label>
                  <Label text="Score Limit" VBox.vgrow="NEVER">
                     <padding>
                        <Insets left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin>
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Label>
                  <Label text="Time Limit" VBox.vgrow="NEVER">
                     <padding>
                        <Insets left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin>
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
            <VBox HBox.hgrow="NEVER">
               <children>
                  <Label fx:id="mapLabel" text="some map" VBox.vgrow="NEVER">
                     <padding>
                        <Insets left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label fx:id="modeLabel" text="some mode" VBox.vgrow="NEVER">
                     <padding>
                        <Insets left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label fx:id="scoreLabel" text="score" textAlignment="RIGHT" VBox.vgrow="NEVER">
                     <padding>
                        <Insets left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label fx:id="timeLabel" text="time" textAlignment="RIGHT" VBox.vgrow="NEVER">
                     <padding>
                        <Insets left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin>
                  </Label>
               </children>
            </VBox>
         </children>
         <effect>
            <SepiaTone />
         </effect>
      </HBox>
      <TableView fx:id="playersTable" minHeight="-Infinity" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="name" editable="false" prefWidth="135.0" text="Name" />
          <TableColumn fx:id="steamID" editable="false" prefWidth="125.0" text="Steam ID" />
          <TableColumn fx:id="ipV4" editable="false" prefWidth="85.0" style="-fx-alignment: CENTER_RIGHT;" text="IP Address" />
          <TableColumn fx:id="latency" editable="false" prefWidth="65.0" style="-fx-alignment: CENTER_RIGHT;" text="Latency" />
          <TableColumn fx:id="score" editable="false" prefWidth="65.0" style="-fx-alignment: CENTER_RIGHT;" text="Score" />
          <TableColumn fx:id="deaths" editable="false" prefWidth="65.0" style="-fx-alignment: CENTER_RIGHT;" text="Deaths" />
          <TableColumn fx:id="kills" editable="false" prefWidth="65.0" style="-fx-alignment: CENTER_RIGHT;" text="Kills" />
          <TableColumn fx:id="assists" editable="false" prefWidth="65.0" style="-fx-alignment: CENTER_RIGHT;" text="Assists" />
          <TableColumn fx:id="longestShot" editable="false" prefWidth="65.0" style="-fx-alignment: CENTER_RIGHT;" text="Distance" />
        </columns>
      </TableView>
      <HBox scaleShape="false" SplitPane.resizableWithParent="false">
         <children>
            <Button fx:id="profileButton" mnemonicParsing="false" onAction="#showProfile" text="Profile">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Button>
            <Button fx:id="kickButton" mnemonicParsing="false" onAction="#kick" text="Kick">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Button>
            <Button fx:id="banButton" layoutX="84.0" layoutY="20.0" mnemonicParsing="false" onAction="#ban" text="Ban">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <ListView fx:id="logList" />
      <HBox prefHeight="113.0" prefWidth="838.0" scaleShape="false" SplitPane.resizableWithParent="false">
         <children>
            <Button mnemonicParsing="false" onAction="#clearLog" text="Clear">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Button>
            <Button mnemonicParsing="false" onAction="#clipboard" text="Clipboard">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Button>
         </children>
      </HBox>
  </items>
</SplitPane>
